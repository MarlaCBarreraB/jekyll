<?xml version="1.0" encoding="UTF-8"?>

<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
	
	{% for page in site.pages %}
	
	<url>
		<loc>http://jekyllrb.com{{ page.url }}</loc>
	</url>
	
	{% endfor %}
	
	{% for post in site.posts %}
	<url>
		<loc>http://jekyllrb.com{{ post.url }}</loc>
	</url>
	{% endfor %}

</urlset>