{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "docs/_data/docs_nav.yml config file schema",
  "type": "array",
  "items": {
    "title": "doc entry",
    "description": "The doc entry",
    "type": "object",
    "required": [
      "title",
      "docs"
    ],
    "properties": {
      "title": {
        "description": "The title",
        "type": "string",
        "minLength": 1
      },
      "docs": {
        "description": "The docs",
        "type": "array",
        "uniqueItems": true,
        "items": {
          "type": "object",
          "properties": {
            "link": {
              "type": "string",
              "minLength": 1
            }
          }
        }
      }
    }
  }
}
